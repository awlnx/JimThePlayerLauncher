{
   // Extension ID: cnpfmodefcbjmcgmglgafinooknbblnb
  "name": "JimThePlayerLauncher",
  "version": "0.0.1",
"key": "",


  "manifest_version": 2,
  "description": "Launches video player of your choice to play videos in",
  "icons": {
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },

  "browser_action": {
    "default_icon": "icons/notplay.png",
    "default_title": "Video Launcher"
  },
  "options_ui" : {
    "page": "options/options.html",
    "chrome_style":false
  },
  "permissions": [
    "storage",
    "background",
    "webRequest",
    "clipboardWrite",
    "tabs",
    "https://*/*",
    "http://*/*",
   "nativeMessaging"
  ]
}
